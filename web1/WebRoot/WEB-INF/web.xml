<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>myservlet</servlet-name>
    <servlet-class>web1.myservlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>loginserlet</servlet-name>
    <servlet-class>web1.loginserlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>wqdqew</servlet-name>
    <servlet-class>web1.wqdqew</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>show_stu_info</servlet-name>
    <servlet-class>web1.show_stu_info</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>myservlet</servlet-name>
    <url-pattern>/servlet/myservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>loginserlet</servlet-name>
    <url-pattern>/servlet/loginserlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>wqdqew</servlet-name>
    <url-pattern>/wqdqew</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>show_stu_info</servlet-name>
    <url-pattern>/servlet/show_stu_info</url-pattern>
  </servlet-mapping>

</web-app>